        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(x - obj_player.x &gt; 0) //Look &lt;&lt;
dir = 1;
else
dir = -1;

if(x&gt;640)
isRight = 1;
else
isRight = -1;


if(hp &lt;= 1500 &amp;&amp; rage = 0)
{
    rage = 1;
    
    instance_create(738,192,obj_darkstone);
    instance_create(368,448,obj_darkstone);
    instance_create(1132,448,obj_darkstone);
}
    
if(startpaze = 1)
{
    startpaze = 0;
    random_rullet = 0;
    paze = 1;
}

if(random_rullet = 1)
{
    paze = choose(1,2,3,4,5);
    if(paze = 2 &amp;&amp; summon_filter = 1)
    {
        paze = choose(1,2,3,4,5);
        summon_filter = 0;
    }
    random_rullet = 0;
}

//Paze 1
if(paze = 1 &amp;&amp; ready = 1)
{
    prev_paze = 1;
    shot_delay = 0;
    ready = 0;
    move = choose(0,1);
    if(move = 1)
    {
        if(x&gt;room_width/2)
        {
            x=110;
            y=160;
        }
        else
        {
            x=room_width-110;
            y=160;
        }
        
        instance_create(x,y,obj_tel);   
    }
    alarm[1] = 300;
}

if(paze = 1 &amp;&amp; shot_delay = 0)
{
    shot_delay = 1;
    ID=instance_create(x-50*isRight,y,obj_darkball);
    with (ID)motion_set(point_direction(x,y,obj_player.x+random_range(-1000,1000),obj_player.y+random_range(-700,700)),7);
    alarm[0] = 2;
}
//Paze 2
if(paze = 2 &amp;&amp; ready = 1 &amp;&amp; summon_cool = 0 &amp;&amp; hp &lt;= 1500)
{
    prev_paze = 2;
    ready = 0;
    summon_cool = 1;
    
    instance_create(738,192,obj_darkstone);
    instance_create(368,448,obj_darkstone);
    instance_create(1132,448,obj_darkstone);
    
    alarm[2] = 150;
    alarm[9] = 900;
}
else if(paze = 2 &amp;&amp; hp &gt; 1000 || summon_cool = 1)
{
    alarm[5] = 1;
}

//Paze 3
if(paze = 3 &amp;&amp; ready = 1)
{
    ready = 0;
    prev_paze = 3;
    firewall_po1 = random_range(50,350);
    firewall_po2 = random_range(350,700);
    firewall_po3 = random_range(700,1100);
    firewall_po4 = random_range(1100,1450);
    
    instance_create(firewall_po1,780,obj_firecheck);
    instance_create(firewall_po2,780,obj_firecheck);
    instance_create(firewall_po3,780,obj_firecheck);
    instance_create(firewall_po4,780,obj_firecheck);
    
    alarm[3] = 240;
}


//Paze 4
if(paze = 4 &amp;&amp; ready = 1)
{
    ready = 0;
     move = choose(0,1);
    if(move = 1)
    {
        if(x&gt;room_width/2)
        {
            x=95;
            y=650;
        }
        else
        {
            x=room_width-95;
            y=650;
        }
        
        instance_create(x,y,obj_tel);   
    }
    soul_pulling = 1;
    soul_delay = 0;
    soul_fire = 0;
    alarm[4] = 420;
    alarm[8] = 300;
    prev_paze = 4;
}
if(soul_pulling = 1 &amp;&amp; soul_delay = 0 &amp;&amp; soul_fire = 0)
{
    soul_delay = 1
    instance_create(x+random_range(-400,400),y+random_range(-400,400),obj_darksoul);
    instance_create(x+random_range(-400,400),y+random_range(-400,400),obj_darksoul);
    alarm[7] = 12;
}


//Paze 5
if(paze = 5 &amp;&amp; ready = 1 &amp;&amp; dark_cool = 0)
{
    prev_paze = 5;
    ready = 0;
    dark_cool = 1;
    instance_create(0,0,obj_darkvision);
    sound_play(darkwave);
    alarm[5] = 120;
    alarm[6] = 720;
}
else if(paze = 5 &amp;&amp; ready = 1 &amp;&amp; dark_cool = 1)
{
    alarm[5] = 1;
}
</string>
