else if place_free(x+lengthdir_x(70, 90-90*(-1)), y)
     x += (-1)*max(70-abs(hspeed), 0);

else{
     move_contact_solid(90-90*(-1), max(70-abs(hspeed), 0));
     }
}

else if(global.evade = 1 &amp;&amp; evadeRightdelay = 1)
{
anglesweep = -90+40*(1);
anglerem = 100;
