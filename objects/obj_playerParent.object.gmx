     x += (1)*max(70-abs(hspeed), 0);

else{
     move_contact_solid(90-90*(1), max(70-abs(hspeed), 0));
     }
}

//SinBoom
if(ready = 3)
{
    instance_create(view_xview,view_yview,obj_white);
    ready = 0.5;
}

//Regen Hp
global.hp+=global.power_hp_regen;

//Regen Energy
