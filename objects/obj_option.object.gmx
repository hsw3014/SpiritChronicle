        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(global.tab = "Stat")
{
if(interest = "Attack")
{
    if(global.power_soul &gt;= global.power_atk_demand)
    {
        global.power_atk+=5*global.atk_count;
        global.power_soul-=100*global.atk_count;
        sound_play(Success);
        global.atk_count+=1;
        
        global.power_atk_demand = 100*global.atk_count;
    }
    else
    {
        sound_play(Error);
    }
}

else if(interest = "Vitality")
{
    if(global.power_soul &gt;= global.power_hp_demand)
    {
        global.power_hp+=200*global.vit_count;
        global.hp+=200*global.vit_count;
        global.maxHp+=200*global.vit_count;
        global.power_soul-=80*global.vit_count;
        global.vit_count+=1;
        sound_play(Success);
        
        global.power_hp_demand = 80*global.vit_count;
    }
    else
    {
        sound_play(Error);
    }
}

else if(interest = "Regen")
{
    if(global.power_soul &gt;= global.power_hp_regen_demand)
    {
        global.power_hp_regen+=0.15;
        global.power_soul-=300+global.regen_count;
        global.regen_count+=500;
        sound_play(Success);
        
        global.power_hp_regen_demand = 300+global.regen_count;
    }
    else
    {
        sound_play(Error);
    }
}

else if(interest = "Energy")
{
    if(global.power_soul &gt;= global.power_energy_regen_demand)
    {
        global.power_energy_regen+=0.15;
        global.power_soul-=500+global.energy_count;
        global.energy_count+=700;
        sound_play(Success);
        
        global.power_energy_regen_demand = 500+global.energy_count;
    }
    else
    {
        sound_play(Error);
    }
}

select = interest;
}

else if(global.tab = "Menu")
{
    if(interest3 = "Resume")
        instance_destroy();
    else if(interest3 = "Exit")
    {
        instance_destroy();
        sound_stop_all();
        global.OVER = 1;
        room_goto(Logo);
    }
}
