        <arguments>
          <argument>
            <kind>1</kind>
            <string>if(hp &gt; 0)
{

if(x - obj_player.x &gt; 0) //Look &lt;&lt;
dir = 1;
else
dir = -1;

if(x&gt;640)
isRight = 1;
else
isRight = -1;

if(random_rullet = 1)
{
    paze = choose(1,2,3);
    if(prev_paze = paze)
    {
        paze = choose(1,2,3);
    }
    if(paze = 2 &amp;&amp; summon_filter = 1)
    {
        paze = choose(1,2,3);
        summon_filter = 0;
    }
    //paze = choose(1,2,3,4);
    random_rullet = 0;
}

//Paze 1
if(paze = 1 &amp;&amp; ready = 1)
{
    if(prev_paze = 3)
        instance_create(x,y,obj_tel);
        
    prev_paze = 1;
    ready = 0;
    point_missile = 1;
    bullet_delay = 0;
    if(prev_paze != 3)
    {
        if(isRight = 1)
        path_start(pt_paze1_Right,10,0,1);
        else
        path_start(pt_paze1_Left,10,0,1);
    }
    else
    {
        ran = choose(1,2);
        if(ran = 1)
            path_start(pt_paze1_Right,10,0,1);
        else
            path_start(pt_paze1_Left,10,0,1);
    }   
    alarm[1] = 420;
    alarm[5] = 180;
}

if(paze = 1 &amp;&amp; point_missile = 1 &amp;&amp; bullet_delay = 0)
{
    ID=instance_create(x,y,obj_bullet);
    with (ID)motion_set(point_direction(x,y,obj_player.x,obj_player.y),4);
    bullet_delay = 1;
    alarm[0] = 15;
}

//Paze 2
if(paze = 2 &amp;&amp; ready = 1)
{
    ready = 0;
    prev_paze = 2;
    summon_choose = choose(2,3,4);
    if(summon_choose = 2)
    {
        instance_create(x-80*isRight,y-20,obj_summon);
        instance_create(x-120*isRight,y-40,obj_summon);
    
        instance_create(x-80*isRight,y-20,obj_mon2);
        instance_create(x-120*isRight,y-40,obj_mon2);
    }
    else if(summon_choose = 3)
    {
        instance_create(x-80*isRight,y-20,obj_summon);
    
        instance_create(x-80*isRight,y-20,obj_mon3);
    }
    else if(summon_choose = 4)
    {
        instance_create(x-120*isRight,y-40,obj_summon);
    
        instance_create(x-120*isRight,y-40,obj_mon4);
    }
    
    alarm[2] = 180;
} 

//Paze 3
if(paze = 3 &amp;&amp; ready = 1)
{
    if(prev_paze != 3 &amp;&amp; x!=prev_x)
    instance_create(x,y,obj_tel);
    prev_paze = 3;
    ready = 0;
    meteo_delay = 0;
    wind_delay = 0;
    prev_x = x;
    prev_y = y;
    x=room_width/2;
    y=room_height/2 + 200;
    alarm[3] = 400;
    alarm[6] = 550;
}
if(paze = 3 &amp;&amp; meteo_delay = 0)
{
    meteo_delay = 1;
   ID2=instance_create(random(room_width),-100,obj_meteo);
    with (ID2)motion_set(point_direction(random_range(200,room_width-200),-100,room_width/2+random_range(-20,20),obj_player.y),6);
    alarm[0] = 12;
}
if(paze = 3 &amp;&amp; wind_delay = 0 &amp;&amp; point_distance(x,y,obj_player.x,obj_player.y) &lt; 200)
{
    obj_player.x-=2.5*dir;
    alram[0] = 120;
}

}
</string>
